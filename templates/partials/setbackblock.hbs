<table>
{{#each setbacks as |item|}}
    <tr>
        <td><a data-action="editItem" data-item-id="{{item.id}}">{{item.name}}</a> ({{item.rating}})</td>
        <td><div>
            <h3>{{localize "WUSHU.item.description"}}</h3>
            <prose-mirror name="system.description" 
                            data-document-u-u-i-d="{{item.uuid}}" 
                            value="{{enrichedDescription}}" 
                            collaborate="true" 
                            toggled="false">
                    {{{enrichedDescription}}}
            </prose-mirror>
        </div></td>
        <td><a data-action="deleteItem" data-item-id="{{item.id}}"><i class="fa-solid fa-trash-can"></i></a></td>
    </tr>
{{/each}}
</table>