<table>
{{#each setbacks as |item|}}
    <tr>
        <td><a data-action="editItem" data-item-id="{{item.id}}">{{item.name}}</a> ({{item.rating}})</td>
        <td>{{> "systems/wushu/templates/partials/descriptionblock.hbs"}}</td>
        <td><a data-action="deleteItem" data-item-id="{{item.id}}"><i class="fa-solid fa-trash-can"></i></a></td>
    </tr>
{{/each}}
</table>